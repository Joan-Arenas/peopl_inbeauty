{# {{ dump(field.value) }} #}

{% for image in field.value %}

    <a href="#" class="ea-lightbox-thumbnail" data-featherlight="#{{ image.id }}"
       data-featherlight-close-on-click="anywhere">
        <img src="{{ vich_uploader_asset(image, 'fichierImageJointe') }}" class="img-fluid"
             alt="{{ image.nomPhoto }}" width="150">
    </a>

    <div id="{{ image.id }}" class="ea-lightbox">
        <img src="{{ vich_uploader_asset(image, 'fichierImageJointe') }}" alt="{{ image.nomPhoto }}">
    </div>

{% endfor %}

